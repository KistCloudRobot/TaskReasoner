PREFIXES:
	PREFIX arbi: <http://www.arbi.com//ontologies#>;

PLAN PERFORM loadPlanPackage(){
	ID : "loadPlanPackage"
	PRECONDITION:
		FACT TaskReasoner($taskReasoner);
	BODY:
		$taskReasoner.loadPlanPackage("./TaskReasonerLocalPlan/common");
		$taskReasoner.loadPlanPackage("./TaskReasonerLocalPlan/Demoplan");
		$taskReasoner.loadPlanPackage("./TaskReasonerLocalPlan/BuiltInPolicy");
		
}